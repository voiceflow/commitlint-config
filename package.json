{
  "name": "@voiceflow/commitlint-config",
  "description": "Commitlint config",
  "version": "1.0.1",
  "author": "Voiceflow",
  "bugs": {
    "url": "https://github.com/voiceflow/commitlint-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@commitlint/config-conventional": "^11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@voiceflow/commitlint-config": "^1.0.1",
    "@voiceflow/eslint-config": "^3.0.0",
    "@voiceflow/git-branch-check": "^1.1.3",
    "@voiceflow/prettier-config": "^1.0.5",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.0",
    "eslint": "^7.20.0",
    "fixpack": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.0"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/voiceflow/commitlint-config",
  "keywords": [
    "commitlint"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "@commitlint/cli": "^11.0.0"
  },
  "prettier": "@voiceflow/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/commitlint-config"
  },
  "scripts": {
    "commit": "cz",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint --format json -o sonar/report.json",
    "prepare": "fixpack",
    "test:run": "echo \"Error: no test specified\" && exit 1",
    "test:dependencies": "depcheck"
  }
}
